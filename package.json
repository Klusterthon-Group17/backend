    "express-validator": "^7.0.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "nodemailer": "^6.9.7",
    "objection": "^3.1.2",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
